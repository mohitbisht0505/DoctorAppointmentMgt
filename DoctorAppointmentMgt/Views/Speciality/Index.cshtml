@model List<Speciality>
<div class="container p-3">
    <div class="row p-2">
        <div class="col-6">
            <h2 class="text-primary">
                Specialities
            </h2>
        </div>
        <div class="col-6">
            <a asp-controller="Speciality" asp-action="Create" class="btn btn-primary">Add New Speciality</a>
        </div>
    </div>
</div>
<table class="table table-bordered table-striped">
    <thead>
        <tr>
            <th> Id </th>
            <th> Speciality Name </th>
            
        </tr>
    </thead>
    <tbody>
        @if (Model != null)
        {
            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @item.SpecialityId
                    </td>
                    <td>
                        @item.SpecialityName
                    </td>
                </tr>
            }
        }
        else
        {
            <tr>
            <td>No Data</td>
            </tr>
        }
    </tbody>
</table>

